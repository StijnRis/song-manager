//   url      = env("DATABASE_URL")
// }

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model FileScore {
  id          Int      @id @default(autoincrement())
  file        String
  proficiency Int
  rating      Int
  playedAt    DateTime @default(now())
}
